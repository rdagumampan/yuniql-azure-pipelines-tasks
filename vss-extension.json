{
  "manifestVersion": 1,
  "id": "yuniql-azdevops-extensions",
  "version": "0.5.1",
  "name": "Yuniql",
  "publisher": "rdagumampan",
  "description": "Run database migrations and schema versioning with Yuniql.",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "yuniql",
    "database-migration",
    "database-version-control",
    "database-management",
    "sql",
    "sqlserver",
    "mysql",
    "postgresql",
    "devops"
  ],
  "galleryFlags": [
    "Free",
    "Public"
  ],
  "icons": {
    "default": "icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "files": [
    {
      "path": "images",
      "addressable": true      
    },
    {
      "path": "src/install",
      "packagePath": "install"
    },
    {
      "path": "src/run",
      "packagePath": "run"
    },
    {
      "path": "src/node_modules",
      "packagePath": "install/node_modules"
    },
    {
      "path": "src/node_modules",
      "packagePath": "run/node_modules"
    }
  ],
  "links": {
    "overview": {
      "uri": "https://getyuniql.org"
    },
    "license": {
      "uri": "https://github.com/rdagumampan/yuniql/blob/master/LICENSE.md"
    },
    "repository": {
      "uri": "https://github.com/rdagumampan/yuniql"
    },
    "issues": {
      "uri": "https://github.com/rdagumampan/yuniql/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/rdagumampan/yuniql"
  },
  "badges": [
    {
      "href": "",
      "uri": "",
      "description": "Build Status"
    }
  ],
  "contributions": [
    {
      "id": "install-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "install"
      }
    },
    {
      "id": "run-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "run"
      }
    }
  ]
}